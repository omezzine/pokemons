{
  "name": "pokemons",
  "private": true,
  "version": "1.0.0",
  "description": "pokemons test for mediflash",
  "license": "MIT",
  "scripts": {
    "nest": "nest",
    "next": "cd src/client && next",
    "prebuild": "rimraf dist",
    "build": "NODE_ENV=production nest build && cd src/client && next build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch --preserveWatchOutput",
    "start:debug": "nest start --debug --watch --preserveWatchOutput",
    "lint": "eslint \"{src,test}/**/*.{ts,tsx}\"",
    "test:server:watch": "jest --config ./src/server/jest.config.ts --watch",
    "test:server:cov": "jest --config ./src/server/jest.config.ts --coverage",
    "test:client:watch": "jest --config ./src/client/jest.config.ts --watch",
    "test:client:cov": "jest --config ./src/client/jest.config.ts --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:request": "jest --setupFiles dotenv-flow/config --config ./test/jest-request.json",
    "cy": "cypress run"
  },
  "dependencies": {
    "@babel/runtime": "^7.20.6",
    "@nestjs/common": "^9.1.6",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.1.6",
    "@nestjs/cqrs": "^9.0.1",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/platform-express": "^9.2.1",
    "axios": "^1.2.0",
    "bootstrap": "^5.2.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cli-color": "^2.0.3",
    "commander": "^9.4.1",
    "nestjs-console": "^8.0.0",
    "next": "^13.0.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-paginate": "^8.1.4",
    "react-toastify": "^9.1.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.8.0",
    "ts-morph": "^17.0.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@nestjs/cli": "^9.1.5",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.2.1",
    "@next/eslint-plugin-next": "^13.0.7",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.15",
    "@types/gtag.js": "^0.0.12",
    "@types/jest": "^28.1.8",
    "@types/node": "^18.11.17",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "cypress": "^12.2.0",
    "dotenv-flow": "^3.2.0",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fishery": "^2.2.2",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^29.3.1",
    "prettier": "^2.8.1",
    "sass": "^1.57.1",
    "supertest": "^6.3.3",
    "ts-jest": "^28.0.8",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.1",
    "typescript": "^4.9.4"
  }
}
